{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "status": 204
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}